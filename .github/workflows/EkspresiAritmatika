#include <vector> //NO 1
#include <iostream>
#include <ctype.h>
using namespace std;

int main() {
    string x; getline(cin,x);
    vector<char> ekspresi;
    
    for(long unsigned int i=0; i<x.length(); i++){
        if(x[i] == ' ')
            continue;
        else if(isdigit(x[i])){ 
            while(i<x.length() && isdigit(x[i])){
                ekspresi.push_back(x[i]);
                i++;
            }
            i--;
        }
        else if(x[i] == '-'){
            ekspresi.push_back(x[i]);
            if(i==0){
                if(isdigit(x[i+1])){
                    i++;
                    while(i<x.length() && isdigit(x[i])){
                        ekspresi.push_back(x[i]);
                        i++;
                    }
                    i--;
                } else {
                    ekspresi.push_back('1');
                    ekspresi.push_back(' ');
                    ekspresi.push_back('*');
                }
            } else {
                if(!isdigit(x[i-1]) && x[i+1] != ' '){
                    ekspresi.push_back('1');
                    ekspresi.push_back(' ');
                    ekspresi.push_back('*');
                } 
            }
        } else
            ekspresi.push_back(x[i]);
        ekspresi.push_back(' ');
    }
    cout <<"Print : ";
    for(auto j:ekspresi)
        cout << j;
    return 0;
}




#include <iostream> //NO 2
#include <ctype.h>
#include <stack>
using namespace std;
long unsigned int i;

bool isOp(char a){
	if(a=='+'||a=='-'||a=='*'||a=='/'||a=='%')
		return true;
	return false;
}
int precedence(char a){
	if(a=='+'||a=='-') return 1;
	if(a=='*'||a=='/'||a=='%') return 2;
	return 0;
}
string convert(string masukan){
	stack<char> data;
	string postfix="";
	
	for(i=0; i<masukan.length(); i++){
		if(isdigit(masukan[i])){
			while(i<masukan.length() && isdigit(masukan[i])){
				postfix += masukan[i];
				i++;
			}
			i--;
			postfix += " ";
		}
		else if(masukan[i] == '(')
			data.push(masukan[i]);
		else if(masukan[i] == ')'){
			while(!data.empty() && data.top() != '('){
				postfix += data.top();
				postfix += " ";
				data.pop();
			}
			data.pop();
		} else {
			if(masukan[i] == '-'){
				if(i==0){
					if(isdigit(masukan[i+1])){
						postfix += masukan[i];
						i++;
						while(i<masukan.length() && isdigit(masukan[i])){
							postfix += masukan[i];
							i++;
						}
						i--;
						postfix += " ";
					}
					else{
						postfix += "-1 ";
						data.push('*');
					}
				} else {
					while(!data.empty() && precedence(data.top()) >= precedence(masukan[i]) && !isOp(masukan[i-1])){
						postfix += data.top();
						postfix += " ";
						data.pop();
					}
					if((isdigit(masukan[i+1]) || masukan[i+1]=='(') && (isdigit(masukan[i-1]) || masukan[i-1]==')'))
						data.push(masukan[i]);
					else {
						postfix += "-1 ";
						data.push('*');
					}
				}
			} else {
				while(!data.empty() && precedence(data.top()) >= precedence(masukan[i])){
					postfix += data.top();
					postfix += " ";
					data.pop();
				}
				data.push(masukan[i]);
			}
		}
	}
	while(!data.empty()){
		postfix += data.top();
		postfix += " ";
		data.pop();
	}
	return postfix;
}

int main(){
	string ekspresi; getline(cin,ekspresi);
	string new_ekspresi="";
	for(i=0;i<ekspresi.length();i++){
		if(ekspresi[i]==' '){
			continue;
		
		}
		else{
			new_ekspresi+=ekspresi[i];	
		}		
	}
	cout <<"Print : ";
	cout << convert(new_ekspresi);
	
	return 0;
}
